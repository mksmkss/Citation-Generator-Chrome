{
    "manifest_version": 3,
    "name": "Citation Format Generator",
    "version": "2.0",
    "author": "Masataka Suzuki",
    "description": "Generate citations from web pages",
    "permissions": ["activeTab", "scripting", "clipboardWrite"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/citation-icon-16.png",
      "48": "icons/citation-icon-48.png",
      "128": "icons/citation-icon-128.png"
    }
  }